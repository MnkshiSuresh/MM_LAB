DATA SEGMENT
     ARRAY DW 1111,2222,3333,4444
     COUNT DB 4
     LARGEST DW ?
     ENDS DATA

CODE SEGMENT
     ASSUME CD:CODE,DS:DATA
     START:MOV AX,DATA
     MOV DS,AX
     MOV CL,COUNT
     LEA SI,ARRAY
     MOV BX,[SI]
     INC SI
     INC SI
     AGAIN:CMP [SI],BX
     JL NOCHANGE
     MOV BX,[SI]
     NOCHANGE:INC SI
     INC SIDEC CL
     JNZ AGAIN
     MOV LARGEST,BX
     
     MOV AH,4CH
     INT 21H
     CODE ENDS
     END START
